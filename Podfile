# platform :ios, '9.0'

target 'HAT App Basic' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HAT App Basic
  pod 'HAT-API-for-iOS'
  pod 'KeychainSwift'
  target 'HAT App BasicTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'HAT-API-for-iOS'
    pod 'KeychainSwift'
  end

  target 'HAT App BasicUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'HAT-API-for-iOS'
    pod 'KeychainSwift'
  end

end
